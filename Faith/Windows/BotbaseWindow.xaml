<Window x:Class="Faith.Windows.BotBaseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Faith.Windows"
        xmlns:resource="clr-namespace:Faith.Localization"
        mc:Ignorable="d"
        Title="{x:Static resource:Translations.BOTBASE_NAME}" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Text="{x:Static resource:Translations.BOTBASE_NAME}" />
    </Grid>
</Window>
